// schema.prisma

generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}

datasource db {
  provider = "sqlite"
  url      = "file:./public/banco.db" // Caminho para o seu arquivo SQLite existente
}

model Telegram {
  id        Int @id @default(autoincrement())
  text      String?
  fileUrl   String?
  buttonText String?
  buttonLink String?
  type       String?

}